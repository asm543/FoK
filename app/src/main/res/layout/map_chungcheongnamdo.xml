<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:focusable="true"
    android:focusableInTouchMode="true"
    android:orientation="vertical" >


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="71dp"
            android:background="@drawable/bar7"
            android:orientation="horizontal">

            <Button
                android:id="@+id/btn_map"
                android:layout_width="50dp"
                android:layout_height="40dp"
                android:layout_weight="0"
                android:layout_gravity="center"
                android:layout_marginLeft="15dp"
                android:background="@drawable/btnmap" />

            <Button
                android:id="@+id/btn_home"
                android:layout_width="80dp"
                android:layout_height="50dp"
                android:layout_weight="0"
                android:layout_marginLeft="110dp"
                android:layout_gravity="center"
                android:background="@drawable/mark3"
                />

            <Button
                android:id="@+id/btn_back"
                android:layout_width="51dp"
                android:layout_height="50dp"
                android:layout_gravity="center"
                android:layout_marginLeft="90dp"
                android:layout_weight="0"
                android:background="@drawable/btnback" />
        </LinearLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:background="@drawable/back_chungnam"
                    android:layout_height="match_parent"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="70dp"
                        android:layout_weight="0"
                        android:orientation="horizontal">
                        <EditText
                            android:id="@+id/editText"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="10dp"
                            android:layout_weight="1"
                            android:ems="10"
                            android:layout_marginLeft="10dp"
                            android:inputType="textPersonName"
                            android:text="여행지 또는 축제 검색"
                            android:textColor="#2B2B2B" />

                        <Button
                            android:id="@+id/btnsearch"
                            android:layout_width="40dp"
                            android:layout_height="40dp"
                            android:layout_margin="10dp"
                            android:layout_weight="0"
                            android:background="@drawable/btnsearch" />
                    </LinearLayout>


                    <TextView
                        android:id="@+id/textView"
                        android:layout_marginTop="150dp"
                        android:layout_marginLeft="20dp"
                        android:layout_width="130dp"
                        android:layout_height="40dp"
                        android:background="@drawable/bar12"
                        android:textColor="#2b2b2b"
                        android:textSize="25dp"
                        android:text=" 지도로 찾기" />

                    <LinearLayout
                        android:layout_width="370dp"
                        android:layout_height="450dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="20dp"
                        android:orientation="vertical">

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="match_parent"
                            android:layout_gravity="top"
                            android:background="@drawable/cleanbar3"
                            android:orientation="vertical">

                            <LinearLayout
                                android:layout_width="match_parent"
                                android:layout_height="361dp"
                                android:layout_marginTop="20dp"
                                android:background="@drawable/map_chungnam2"
                                android:orientation="vertical">

                                <LinearLayout
                                    android:layout_width="match_parent"
                                    android:layout_height="match_parent"
                                    android:orientation="vertical">

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="139dp"
                                        android:orientation="horizontal">

                                        <Button
                                            android:id="@+id/btn_cn1"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:layout_marginLeft="130dp"
                                            android:layout_marginTop="75dp"

                                            android:background="@drawable/btnmount" />

                                        <Button
                                            android:id="@+id/btn_cn2"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:layout_marginLeft="75dp"
                                            android:layout_marginTop="30dp"
                                            android:layout_marginRight="50dp"
                                            android:background="@drawable/btndance" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="80dp"
                                        android:orientation="horizontal">

                                        <Button
                                            android:id="@+id/btn_cn3"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:layout_marginLeft="100dp"
                                            android:layout_weight="0"

                                            android:background="@drawable/btnhanwoo" />

                                        <Button
                                            android:id="@+id/btn_cn4"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:layout_weight="0"
                                            android:layout_marginLeft="75dp"

                                            android:background="@drawable/btngochu" />

                                    </LinearLayout>

                                    <LinearLayout
                                        android:layout_width="match_parent"
                                        android:layout_height="match_parent"
                                        android:orientation="horizontal">

                                        <Button
                                            android:id="@+id/btn_cn5"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:layout_weight="0"
                                            android:layout_marginLeft="115dp"

                                            android:background="@drawable/btnkoreantraditionalmusic" />

                                        <Button
                                            android:id="@+id/btn_cn6"
                                            android:layout_width="40dp"
                                            android:layout_height="40dp"
                                            android:layout_weight="0"
                                            android:layout_marginLeft="100dp"

                                            android:background="@drawable/btngoguma" />

                                    </LinearLayout>
                                </LinearLayout>
                            </LinearLayout>
                        </LinearLayout>


                    </LinearLayout>


                    <TextView
                        android:id="@+id/textView12"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="20dp"
                        android:layout_width="130dp"
                        android:layout_height="40dp"
                        android:background="@drawable/bar12"
                        android:textColor="#2b2b2b"
                        android:textSize="25dp"
                        android:text=" 추천 축제" />

                    <HorizontalScrollView
                        android:layout_width="match_parent"
                        android:layout_height="340dp"
                        android:layout_margin="20dp"
                        android:background="@drawable/bar12">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="330dp"
                                android:layout_margin="10dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/bar12"
                                android:orientation="vertical">

                                <ImageButton
                                    android:id="@+id/festival1"
                                    android:layout_margin="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="fitXY"
                                    app:srcCompat="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/txt_festival_title1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="20dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_festival_where1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_festival_date1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="330dp"
                                android:layout_margin="10dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/bar12"
                                android:orientation="vertical">

                                <ImageButton
                                    android:id="@+id/festival2"
                                    android:layout_margin="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="fitXY"
                                    app:srcCompat="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/txt_festival_title2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="20dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_festival_where2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_festival_date2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="330dp"
                                android:layout_margin="10dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/bar12"
                                android:orientation="vertical">

                                <ImageButton
                                    android:id="@+id/festival3"
                                    android:layout_margin="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="fitXY"
                                    app:srcCompat="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/txt_festival_title3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="20dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_festival_where3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_festival_date3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />
                            </LinearLayout>
                        </LinearLayout>
                    </HorizontalScrollView>

                    <TextView
                        android:id="@+id/textView1"
                        android:layout_marginTop="10dp"
                        android:layout_marginLeft="20dp"
                        android:layout_width="130dp"
                        android:layout_height="40dp"
                        android:background="@drawable/bar12"
                        android:textColor="#2b2b2b"
                        android:textSize="25dp"
                        android:text=" 추천 여행지" />

                    <HorizontalScrollView
                        android:layout_width="match_parent"
                        android:layout_height="340dp"
                        android:layout_margin="20dp"
                        android:background="@drawable/bar12">

                        <LinearLayout
                            android:layout_width="wrap_content"
                            android:layout_height="match_parent"
                            android:orientation="horizontal">

                            <LinearLayout
                                android:layout_width="330dp"
                                android:layout_margin="10dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/bar12"
                                android:orientation="vertical">

                                <ImageButton
                                    android:id="@+id/tour1"
                                    android:layout_margin="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="fitXY"
                                    app:srcCompat="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/txt_tour_title1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="20dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_tour_where1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_tour_date1"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="330dp"
                                android:layout_margin="10dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/bar12"
                                android:orientation="vertical">

                                <ImageButton
                                    android:id="@+id/tour2"
                                    android:layout_margin="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="fitXY"
                                    app:srcCompat="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/txt_tour_title2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="20dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_tour_where2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_tour_date2"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />
                            </LinearLayout>

                            <LinearLayout
                                android:layout_width="330dp"
                                android:layout_margin="10dp"
                                android:layout_height="match_parent"
                                android:background="@drawable/bar12"
                                android:orientation="vertical">

                                <ImageButton
                                    android:id="@+id/tour3"
                                    android:layout_margin="10dp"
                                    android:layout_width="match_parent"
                                    android:layout_height="200dp"
                                    android:scaleType="fitXY"
                                    app:srcCompat="@mipmap/ic_launcher" />

                                <TextView
                                    android:id="@+id/txt_tour_title3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:textSize="20dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_tour_where3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />

                                <TextView
                                    android:id="@+id/txt_tour_date3"
                                    android:layout_width="match_parent"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="10dp"
                                    android:layout_marginTop="10dp"
                                    android:textSize="15dp"
                                    android:textColor="#ffffff"
                                    android:text="TextView" />
                            </LinearLayout>
                        </LinearLayout>
                    </HorizontalScrollView>
                </LinearLayout>

            </LinearLayout>
        </ScrollView>


    </LinearLayout>
</LinearLayout>